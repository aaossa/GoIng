<p id="notice"><%= notice %></p>

<p><strong>Name:</strong> <%= @course.name %></p>

<p><strong>Teaching assistants:</strong></p>
<ul>
  <% @course.teaching_assistants.each do |teaching_assistant| %>
  	<li><%= link_to teaching_assistant.name, teaching_assistant_path(teaching_assistant) %></li>
  <% end %>
</ul>

<p><strong>Available modules:</strong></p>
<ul>
  <% @course.available_modules.each do |time_block| %>
    <li><%= link_to time_block.display_block, time_block_path(time_block) %></li>
  <% end %>
</ul>


<%= link_to 'Edit', edit_course_path(@course) %> |
<%= link_to 'Back', courses_path %>
